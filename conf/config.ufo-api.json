{
    "host": "",
    "port": 4128,
    "task": {
        "path": "/tasks",
        "cmd": "dufo '${sampleFile}' '${taskDir}' --delete-sample > '${taskDir}.log' 2>&1 && cat '${taskDir}/result.json'",
        "dir": "tasks",
        "incomingDir": "tasks-incoming"
    },
    "report": {
        "method": "api",
        "api": {
            "host": "http://localhost/mq.php"

        },
        "amq": {
            "host": "",
            "port": 61613,
            "queue": "csfResult"
        }
    },
    "log": {
        "format": "[:date[Asia/Taipei]] :remote-addr - :remote-user ${yellow(':method')} :url HTTP/:http-version ${green(':status')} :res[content-length] ms",
        "dateFormat": "YYYY-MM-DD HH:mm:ss.SSS"
    }
}
