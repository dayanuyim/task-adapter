{
    "host": "",
    "port": 4128,
    "task": {
        "path": "/tasks",
        "dir": "tasks",
        "create": {
            "cmd": "dufo '${sampleFile}' '${taskDir}' --delete-sample 2> '${taskDir}.log' | sed -n '/^{/,/^}/p'"
        },
        "delete": {
            "cmd": "docker stop 'ufo-cmd-${taskId}'"
        }
    },
    "report": {
        "method": "api",
        "api": {
            "host": "http://localhost/mq.php"

        },
        "amq": {
            "host": "",
            "port": 61613,
            "queue": "csfResult"
        }
    },
    "log": {
        "format": "[:date[Asia/Taipei]] :remote-addr - :remote-user ${yellow(':method')} :url HTTP/:http-version ${green(':status')} :res[content-length] ms",
        "dateFormat": "YYYY-MM-DD HH:mm:ss.SSS"
    }
}
