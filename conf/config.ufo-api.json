{
    "host": "",
    "port": 4128,
    "task": {
        "path": "/tasks",
        "dir": "/opt/ufo/tasks",
        "create": {
            "cmd": "dufo cmd ${toDockerEnv(params)} '${sampleFile}' '${outputDir}' --delete-sample 2> '${outputDir}.log' | sed -n '/^{/,/^}/p'"
        },
        "delete": {
            "cmd": "bash -c 'docker rm -f ufo-cmd-${taskId} || true'"
        }
    },
    "report": {
        "method": "api",
        "sync": {
        },
        "api": {
            "host": "http://localhost/ufo/mq.php"

        },
        "amq": {
            "host": "",
            "port": 61613,
            "queue": "csfResult"
        }
    },
    "log": {
        "format": "[:date[Asia/Taipei]] :remote-addr - :remote-user ${yellow(':method')} :url HTTP/:http-version ${green(':status')} :res[content-length] ms",
        "dateFormat": "YYYY-MM-DD HH:mm:ss.SSS"
    }
}
